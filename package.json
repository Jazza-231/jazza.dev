      "eslint": "^9.20.1",
      "eslint-config-prettier": "^10.0.1",
      "eslint-plugin-svelte": "^2.46.1",
      "globals": "^15.15.0",
      "prettier": "^3.5.1",
      "prettier-plugin-svelte": "^3.3.3",
      "sharp": "^0.33.5",
      "svelte": "5.20.1",
      "svelte-check": "^4.1.4",
